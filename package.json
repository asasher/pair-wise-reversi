{
  "name": "ap-assign-3",
  "version": "1.0.0",
  "scripts": {
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "app": "nodemon --harmony server/main.js",
    "start": "tsc && npm run app",
    "browser-sync": "browser-sync start -p \"localhost:8080\" -f \"client\"",
    "dev": "tsc && concurrently \"npm run tsc:w\" \"npm run app\" \"npm run browser-sync\"",
    "typings": "typings",
    "postinstall": "typings install"
  },
  "dependencies": {
    "angular2": "^2.0.0-beta.15",
    "co": "^4.6.0",
    "es6-shim": "^0.35.0",
    "jade": "^1.11.0",
    "koa": "^1.2.0",
    "lodash": "^4.11.1",
    "mz": "^2.4.0",
    "node-uuid": "^1.4.7",
    "nodemon": "^1.9.1",
    "promised-mongo": "^1.2.0",
    "reflect-metadata": "^0.1.3",
    "rxjs": "^5.0.0-beta.6",
    "socket.io": "^1.4.5",
    "socket.io-client": "^1.4.5",
    "systemjs": "^0.19.26",
    "zone.js": "^0.6.11",
    "typescript": "latest",
    "typings": "latest"
  },
  "devDependencies": {
    "browser-sync": "latest",
    "concurrently": "latest",
  }
}